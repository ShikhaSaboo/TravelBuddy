<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.minorproject.ckaa.travelbuddy" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/travel_buddy"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".permits_activity"
            android:label="@string/title_activity_permits_activity" >
        </activity>
        <activity
            android:name=".Accomodation"
            android:label="@string/title_activity_accomodation" >
        </activity>
        <activity
            android:name=".Connection"
            android:label="@string/title_activity_connection" >
        </activity>
        <activity
            android:name=".CulturalEttiquete"
            android:label="@string/title_activity_cultural_ettiquete" >
        </activity>
        <activity
            android:name=".Communication"
            android:label="@string/title_activity_communication" >
        </activity>
        <activity
            android:name=".HealthGuide"
            android:label="@string/title_activity_health_guide" >
        </activity>
        <activity
            android:name=".NationalMuseum"
            android:label="@string/title_activity_national_museum" >
        </activity>
        <activity
            android:name=".Narayanhiti"
            android:label="@string/title_activity_narayanhiti" >
        </activity>
        <activity
            android:name=".Durbar_Square"
            android:label="@string/title_activity_durbar__square" >
        </activity>
        <activity
            android:name=".Pashupatinath"
            android:label="@string/title_activity_pashupatinath" >
        </activity>
        <activity
            android:name=".Swyambhu"
            android:label="@string/title_activity_swyambhu" >
        </activity>
        <activity
            android:name=".Gardendreams"
            android:label="@string/title_activity_gardendreams" >
        </activity>
    </application>

</manifest>
